{
  "name": "instagram",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup-project": "npm run setup-client && npm run setup-server",
    "build-client": "cd instagram/client && npm run build",
    "setup-client": "cd instagram/client && npm install &&  npm run build",
    "setup-server": "npm install && pip install -r requirements.txt",
    "server": "python manage.py runserver",
    "server-ubuntu": "python manage.py runserver",
    "client": "npm start --prefix instagram/client",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start-ubuntu": "concurrently --kill-others-on-fail \"npm run server-ubuntu\" \"npm run client\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^7.6.0"
  }
}
